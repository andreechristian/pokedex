{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
		"semicolon": [
			true,
			"never"
		],
		"no-namespace": false,
		"variable-name": {
			"options": [
				"ban-keywords",
				"check-format",
				"allow-leading-underscore",
				"allow-pascal-case"
			]
		}
	},
    "rulesDirectory": []
}